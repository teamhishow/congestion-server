        </ul>
        <form class="form-inline my-2 my-lg-0">
          <input class="form-control mr-sm-2" type="text" placeholder="Search" aria-label="Search">
          <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
        </form>
      </div>
    </nav>

    <main role="main" class="container">

      <div class="starter-template">

    <!--<img border="0" src="/next_station.png" width="128" height="128" alt="行先">-->

    <h2>列車ID <%=train_id %> (<%=time %>ごろ、"<%=station %>"&nbsp;を出ました)</h2>
    <br />
    <br />
    <br />
    <img border="0" src="/next_station.png" width="550" alt="行先">
    <!-- コードから渡されたデータを表示-->
    <br />
    <br />
    <br />
    <br />
    <p>
    <% var ary = congestions %>

    <% ary.forEach(function (doors, key) { %>
        <span style="padding: 10px; margin-bottom: 10px; border: 1px solid #333333;">
        <%=key+1 %>
        <% doors.forEach(function (door, door_key) { %>
            <span style="color:#<%= door %>;">■</span>
        <% }) %>
        </span>
        &nbsp;
    <% }); %>
    </p>
      </div>

    </main><!-- /.container -->

    <!-- Bootstrap core JavaScript
    ================================================== -->
